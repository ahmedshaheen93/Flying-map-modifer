<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="208.0" prefWidth="505.0" stylesheets="@../css/style.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.shaheen.contoller.ModifyAttrViewController">
    <children>
        <Pane id="title-pane" prefHeight="64.0" prefWidth="505.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <Label fx:id="messageTitleLabel" layoutX="14.0" layoutY="17.0" prefHeight="30.0" prefWidth="478.0"
                   text="Edit Values" textFill="WHITE">
                <font>
                    <Font size="18.0"/>
                </font>
            </Label>
        </Pane>
        <Pane layoutY="64.0" prefHeight="82.0" prefWidth="505.0">
            <children>
                <GridPane layoutX="14.0" layoutY="14.0" prefHeight="70.0" prefWidth="478.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="23.0" minHeight="10.0" prefHeight="23.0" vgrow="SOMETIMES"/>
                        <RowConstraints maxHeight="28.0" minHeight="10.0" prefHeight="23.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label prefHeight="21.0" prefWidth="26.0" text="X" fx:id="xLable">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                        <Label fx:id="YLable" prefHeight="21.0" prefWidth="26.0" text="Y" GridPane.columnIndex="1">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                        <Label fx:id="zLable" prefHeight="21.0" prefWidth="26.0" text="Z" GridPane.columnIndex="2">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                        <Label fx:id="typeLable" prefHeight="21.0" prefWidth="54.0" text="Type"
                               GridPane.columnIndex="3">
                            <font>
                                <Font size="18.0"/>
                            </font>
                        </Label>
                        <Spinner fx:id="x"
                                 GridPane.columnIndex="0" GridPane.rowIndex="1" editable="true"/>

                        <Spinner fx:id="y" GridPane.columnIndex="1" GridPane.rowIndex="1" editable="true"/>

                        <Spinner fx:id="z" GridPane.columnIndex="2" GridPane.rowIndex="1" editable="true"/>

                        <TextField fx:id="type" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
                    </children>
                </GridPane>
            </children>
        </Pane>
        <Pane layoutX="0" layoutY="165.0" prefHeight="50.0" prefWidth="505.0">
            <children>
                <Button fx:id="okButton" layoutX="103.0" layoutY="10.0" onAction="#onOkAction" prefHeight="30"
                        prefWidth="100" text="Ok"/>
                <Button fx:id="cancelButton" layoutX="302.0" layoutY="10.0" onAction="#onCancelAction" prefHeight="30"
                        prefWidth="100" text="Cancel"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
